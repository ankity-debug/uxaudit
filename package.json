{
  "name": "lycheelens-audit-platform",
  "version": "1.0.0",
  "description": "lycheeLens - AI-powered UX audit platform for instant design insights",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "cd backend && npm start"
  },
  "keywords": [
    "ux",
    "audit",
    "ai",
    "gemini",
    "design"
  ],
  "author": "lycheeLens",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "@types/multiparty": "^4.2.1",
    "@types/uuid": "^10.0.0",
    "@vercel/node": "^5.3.21",
    "axios": "^1.11.0",
    "multiparty": "^4.2.3",
    "uuid": "^12.0.0"
  }
}
